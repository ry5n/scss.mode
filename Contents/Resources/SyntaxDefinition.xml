        <keywords id="CSSCommentHeadings" color="#222" font-weight="bold">
          <regex>(# .*)([\r\n]|\*/)</regex>
        </keywords>
      </state>
      
      <state id="SassComment" type="comment" color="#737373" font-weight="normal" font-style="normal">
        <begin><regex>//</regex></begin>
        <end><regex>[\n\r]</regex></end>
        
        <!-- Highlight ad-hoc comment headings. Not part of any standard -->
        <keywords id="SassCommentHeadings" color="#222" font-weight="bold" font-style="normal">
          <regex>(# .*)[\r\n]</regex>
        </keywords>
        
        <!-- Highlight ad-hoc comment tags.
             Inspired by PHPDoc, but not all PHPDoc tags are supported.
             -->
        <keywords id="SassCommentTags" color="#b23200" font-weight="bold" font-style="normal">
          <string>@todo</string>
        </keywords>
      </state>
      
